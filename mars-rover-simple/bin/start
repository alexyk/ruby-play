#!/usr/bin/env ruby

# add lib folder
$LOAD_PATH.unshift File.dirname(__FILE__) + '/../lib'


require "main"
require "print/version"

if ARGV.count == 0 or ['help', '--help', "?", "/?"].include? ARGV[0]
  require "print/help"
else
  require "processor/exec"
  require "print/input"
  require "print/result"
end